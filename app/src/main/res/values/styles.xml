<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">#c56000</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:textColorSecondary">#000000</item>
        <item name="android:textColorSecondaryInverse">@color/secondary_text_material_dark</item>

    </style>

    <style name="dialog_data">
        <item name="android:textColor">@color/button_off</item>
        <item name="android:textSize">15sp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingLeft">20dp</item>

    </style>

    <style name="Theme">tema</style>

    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

</resources>
